{
  "name": "riderize",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "android:debug": "npx react-native run-android --variant=debug",
    "android:staging": "npx react-native run-android --variant=stagingrelease",
    "android:prod": "npx react-native run-android --variant=release",
    "ios": "npx react-native run-ios",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:ios-debug": "detox build --configuration ios:debug",
    "build:ios-release": "detox build --configuration ios:release",
    "build:android-debug": "detox build --configuration android:debug",
    "build:android-release": "detox build --configuration android:release",
    "test:ios-debug": "detox test --configuration ios:debug",
    "test:ios-release": "detox test --configuration ios:release",
    "test:android-debug": "detox test --configuration android:debug",
    "test:android-release": "detox test --configuration android:release",
    "test:android-release-ci": "detox test --configuration android:release -l verbose --headless --record-logs all --take-screenshots all",
    "e2e:ios-debug": "npm run build:ios-debug && npm run test:ios-debug",
    "e2e:ios-release": "npm run build:ios-release && npm run test:ios-release",
    "e2e:android-debug": "npm run build:android-debug && npm run test:android-debug",
    "e2e:android-release": "npm run build:android-release && npm run test:android-release",
    "clean:android": "pushd android && ./gradlew clean && popd",
    "introspect": "graphql-codegen --config codegen.yml",
    "schemas": "graphql-codegen --config codegen.yml",
    "schemas:watch": "npm run schemas -- --watch",
    "codepush:sync": "appcenter codepush release-react -a Riderize/Riderize-Staging -d Staging -m -t 'v1.0.0 - v2.2.110' && appcenter codepush release-react -a Riderize/Riderize-Android -d Staging -m -t 'v1.0.0 - v2.2.110'",
    "codepush:sync:prod": "appcenter codepush release-react -a Riderize/Riderize-Staging -d Production && appcenter codepush release-react -a Riderize/Riderize-Android -d Production",
    "codepush:sync:prod:m": "appcenter codepush release-react -a Riderize/Riderize-Staging -d Production -m && appcenter codepush release-react -a Riderize/Riderize-Android -d Production -m"
  },
